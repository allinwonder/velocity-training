{
  "id": "webserver",
  "cpus": 0.5,
  "mem": 32.0,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "nginx:latest",
      "network": "BRIDGE",
      "portMappings": [ { 
        "containerPort": 80,
        "hostPort": 0
       }
      ]
    },
    "volumes": [{
      "containerPath": "/usr/share/nginx/html",
      "hostPath": "/tmp/gen",
      "mode": "RO"
     }
    ]
  },
  "acceptedResourceRoles": [
      "slave_public"
  ]
}